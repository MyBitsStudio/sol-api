model TokenBuy {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenName    String
  tokenAddress String   @unique
  tag          String
  bots         String[]
  expires      DateTime
  gif          String
  link         String

  @@map("TokenBuy")
}

model Trending {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenName    String
  tokenAddress String
  sol          Float
  expiresAt    DateTime

  @@map("Trending")
}

model Shill {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenName    String
  tokenAddress String
  channels     String[]
  expiresAt    DateTime
  message      String

  @@map("Shill")
}

model BotChannels {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  tokenKey  String   @unique
  channels  String[]

  @@map("BotChannels")
}

model TrendPosition {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenAddress String   @unique
  tokenName    String
  tag          String
  link         String
  position     Int
  price        String
  cap          Int

  @@map("TrendPosition")
}

model TrendingQueue {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenAddress String   @unique
  time         Int
  tokenName    String
  tag          String
  telegram     String
  link         String

  @@map("TrendingQueue")
}

model Ads {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  address   String   @unique
  text      String
  url       String
  expiresAt DateTime

  @@map("Ads")
}

model AdQueue {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  address   String   @unique
  text      String
  url       String
  telegram  String
  time      Int

  @@map("AdQueue")
}

model Prices {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  type   String
  prices String[]

  @@map("Prices")
}

model BuyBotWaiting {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenName    String
  tokenAddress String
  tag          String
  owner        String
  telegram     String
  link         String
  channel      String

  @@map("BuyBotWaiting")
}

model PinPost {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  tokenName    String
  tokenAddress String   @unique
  tag          String
  message      String
  telegram     String

  @@map("PinPost")
}
